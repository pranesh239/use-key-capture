  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --coverage && open coverage/Icov-report/index.html",
