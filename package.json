    "@testing-library/react-hooks": "^3.2.1",
    "docz": "^2.2.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.1",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18 || ^19",
    "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
  },
  "browserslist": "> 0.25%, not dead",
